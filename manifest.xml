<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="TaskPaneApp">
  <Id>12345678-1234-1234-1234-123456789012</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Your Company</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Excel SQL Assistant"/>
  <Description DefaultValue="Convert natural language to SQL and interact with Excel."/>
  <IconUrl DefaultValue="https://localhost:3000/assets/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://localhost:3000/assets/icon-80.png"/>
  <SupportUrl DefaultValue="https://yourcompany.com/support"/>
  <AppDomains>
    <AppDomain>http://localhost:3000</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>
  <Requirements>
    <Sets DefaultMinVersion="1.8">
      <Set Name="ExcelApi"/>
    </Sets>
  </Requirements>
  <DefaultSettings>
    <SourceLocation DefaultValue="http://localhost:3000/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <WebApplicationInfo>
    <Id>__AAD_APP_ID__</Id>
    <Resource>api://__AAD_APP_ID__</Resource>
    <Scopes>
      <Scope>Files.ReadWrite.All</Scope>
      <Scope>User.Read</Scope>
      <Scope>openid</Scope>
      <Scope>profile</Scope>
    </Scopes>
  </WebApplicationInfo>
</OfficeApp> 